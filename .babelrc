{
    "presets": [
        [
            "@babel/preset-env", 
            {
                "modules": "commonjs",
                "useBuiltIns": "usage"
            }
        ],
        "@babel/preset-typescript"
    ],
    "plugins": [
        "@babel/plugin-transform-object-assign",
        ["@babel/plugin-transform-runtime", { "corejs": 2 }],
        "@babel/plugin-transform-regenerator"
    ]
}